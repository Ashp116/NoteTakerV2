{
  "name": "notetaker",
  "version": "1.0.2",
  "main": "./out/main.js",
  "scripts": {
    "test": "nodemon --watch . --exec \"electron .\"",
    "start": "npm install && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Ashp116",
  "license": "ISC",
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@types/express": "^4.17.13",
    "@types/tinymce": "^4.6.5",
    "convertapi": "^1.11.2",
    "docx-pdf": "^0.0.1",
    "express": "^4.18.1",
    "html-pdf-node": "^1.0.7",
    "html-to-docx": "^1.4.0",
    "tinymce": "^6.2.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "electron": "^20.1.3",
    "electron-builder": "^23.3.3"
  },
  "build": {
    "appId": "com.electron.NoteTaker",
    "extraMetadata": {
      "name": "Note Taker",
      "description": "A personal project, created to better the author's knowledge on electronJS",
      "projectUrl": "https://github.com/Ashp116/NoteTakerV2#readme",
      "license": "MIT",
      "author": "Ashp116",
      "repository": "https://github.com/Ashp116/NoteTakerV2"
    },
    "productName": "Note Taker",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
