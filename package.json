    "copyright": "Copyright Â©2022 Ashp116}",
    "fileAssociations": [
      {
        "ext": "note",
        "name": "Note Taker",
        "role": "Notebook",
        "icon": "build/note-file.ico"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashp116/NoteTakerV2.git"
  },
  "bugs": {
    "url": "https://github.com/Ashp116/NoteTakerV2/issues"
  },
